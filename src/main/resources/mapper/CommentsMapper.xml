<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.logether.comments.mapper.CommentsMapper">

    <insert id="CommentsAdminAdd" parameterType="board">
        INSERT INTO comments
            (board_code, user_id, comts_contents)
        VALUES
            (#{boardCode}, #{userId}, #{comtsContents})
    </insert>

</mapper>
